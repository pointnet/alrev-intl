<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net5.0</TargetFrameworks>
    <LangVersion>9.0</LangVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>alrev-intl.snk</AssemblyOriginatorKeyFile>
    <AssemblyName>Alrev.Intl.RelativeTime.DependencyInjection</AssemblyName>
    <PackageId>Alrev.Intl.RelativeTime.DependencyInjection</PackageId>
    <PackageVersion>1.0.1</PackageVersion>
    <Authors>pointnet</Authors>
    <Company>Alrev.Intl</Company>
    <Description>Relative Time Dependency Injection library of Alrev.Intl</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/pointnet/alrev-intl/tree/master/packages/Alrev.Intl.RelativeTime.DependencyInjection</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageIconUrl>icon.png</PackageIconUrl>
    <PackageTags>intl;cldr;icu;internationalization;globalization;localization;relativetime</PackageTags>
    <RepositoryUrl>https://github.com/pointnet/alrev-intl.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Alrev.Intl.PluralRules.DependencyInjection\Alrev.Intl.PluralRules.DependencyInjection.csproj" />
    <ProjectReference Include="..\Alrev.Intl.RelativeTime\Alrev.Intl.RelativeTime.csproj" />
  </ItemGroup>
    
  <ItemGroup>
    <None Include="..\..\icon.png" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
